<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andersenlab2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndersenLab2">
        <activity
            android:name=".ImplicitIntents"
            android:exported="false" />
        <activity
            android:name=".ImplicitIntentReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="developer.android.com"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityLifecCycleStateSecond"
            android:exported="false" />
        <activity
            android:name=".ActivityLifeCycleState"
            android:exported="false" />
        <activity
            android:name=".SecondActivity"
            android:exported="false" />
        <activity
            android:name=".FirstActivity"
            android:exported="false" />
        <activity
            android:name=".TextAndScrollingViews"
            android:exported="false" />
        <activity
            android:name=".TheLayoutEditorLinearLayout"
            android:exported="false" />
        <activity
            android:name=".YourFirstInteractiveUI"
            android:exported="false" />
        <activity
            android:name=".AndroidStudioHelloWorld"
            android:exported="false" />
        <activity
            android:name=".TheEditLayoutRelative"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>